{
  "name": "finance-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./src/index.css -o ../backend/finance_project/static/tailwind.css --minify",
    "build:js": "esbuild src/index.jsx --bundle --outfile=../backend/finance_project/static/app.js --minify --sourcemap --loader:.jsx=jsx --jsx=automatic --jsx-import-source=react",
    "build": "npm run build:css && npm run build:js"
  },
  "dependencies": {
    "axios": "1.6.8",
    "chart.js": "4.4.1",
    "clsx": "2.0.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "lucide-react": "0.529.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.7",
    "autoprefixer": "10.4.16",
    "esbuild": "0.19.11",
    "postcss": "8.4.33",
    "tailwindcss": "3.4.1"
  }
}

